{
  "name": "@abbasfarid/react-native-video",
  "version": "1.0.0",
  "private": false,
  "description": "React Native Video Player",
  "author": "Abbas Farid",
  "license": "MIT",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abbasfreestyle/react-native-video.git"
  },
  "scripts": {
    "android": "react-native run-android",
    "clean:ios:cache": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf node_modules/ && yarn cache clean && yarn",
    "clean:ios:derived-data": "rm -rf ios/build && rm -rf ~/Library/Developer/Xcode/DerivedData",
    "clean:ios": "yarn clean:ios:cache && yarn clean:ios:derived-data",
    "clean:packager": "yarn start -- --reset-cache",
    "clean": "yarn clean:android && yarn clean:ios",
    "generate:assets": "cp -a src/@abbasfarid/react-native-video/assets dist/assets",
    "generate": "yarn tsc --project tsconfig.build.json",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "pod:install": "cd ios && pod install",
    "pod": "yarn && yarn pod:install",
    "prettier:check": "prettier 'src/**/*.{ts,tsx,json,css,scss}' --check",
    "prettier:write": "prettier 'src/**/*.{ts,tsx,json,css,scss}' --write",
    "semantic-release": "semantic-release",
    "start": "react-native start",
    "storybook:serve": "start-storybook -p 7007",
    "storybook": "rnstl",
    "test": "yarn jest",
    "type": "yarn tsc"
  },
  "peerDependencies": {
    "react-native-keep-awake": "^4.x.x",
    "react-native-orientation": "^3.x.x",
    "react-native-reanimated": "^2.x.x",
    "react-native-video": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/eslint-config": "^3.0.1",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "@storybook/addon-actions": "^5.3",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/addon-storyshots": "^6.3.8",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@testing-library/jest-native": "^4.0.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "^27.0.1",
    "@types/react": "^17.0.19",
    "@types/react-native": "^0.64.13",
    "@types/react-native-video": "^5.0.8",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "babel-jest": "^27.2.4",
    "babel-loader": "^8.2.2",
    "eslint": "7.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.5",
    "husky": "^7.0.2",
    "jest": "^27.2.4",
    "metro-react-native-babel-preset": "^0.66.0",
    "prettier": "^2.4.0",
    "react": "^17.0.2",
    "react-dom": "17.0.2",
    "react-native": "^0.65.1",
    "react-native-codegen": "^0.0.7",
    "react-native-keep-awake": "^4.0.0",
    "react-native-orientation": "^3.1.3",
    "react-native-reanimated": "^2.3.0-beta.1",
    "react-native-safe-area-context": "^3.3.0",
    "react-native-screens": "^3.6.0",
    "react-native-storybook-loader": "^2.0.4",
    "react-native-svg": "^12.1.1",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-video": "^5.1.1",
    "react-test-renderer": "17.0.2",
    "semantic-release": "^17.4.7",
    "typescript": "^4.4.2"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src",
        "./storybook"
      ],
      "pattern": "**/*.stories.{ts,tsx}",
      "outputFile": "./storybook/storyLoader.js"
    }
  }
}
